pipeline {
   agent any
   environment {
       DISABLE_AUTH = 'true'
       DEPLOY_TO = 'production'
   }
   stages {
       stage(“Build”) {
           environment {
                   ENABLE_AUTH = 'false’                           
              }
           steps {
               echo env.DISABLE_AUTH
               echo env.ENABLE_AUTH
               echo "${DEPLOY_TO}" 
               sh 'printenv'
           }
       }
   }
}
